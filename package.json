{
  "name": "mcp-server",
  "version": "1.0.0",
  "description": "Model Context Protocol Server for Blender and Unreal Engine 5",
  "main": "install.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "install": "node install.js",
    "start": "python run_mcp.py all",
    "start:server": "python run_mcp.py server",
    "start:blender": "python run_mcp.py blender",
    "start:ue5": "python run_mcp.py ue5"
  },
  "bin": {
    "mcp-install": "./install.js"
  },
  "keywords": [
    "mcp",
    "blender",
    "unreal",
    "ue5",
    "ai",
    "automation"
  ],
